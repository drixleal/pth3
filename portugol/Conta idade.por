programa {
  funcao inicio() 
  {
    inteiro idade
    escreva ("Digite a idade: ")
    leia(idade)

    se (idade >= 18) 
    {
      escreva ("Você foi aprovade! \n")
    }
    senao 
    {
      escreva ("Você não foi aprovade! \n")
    }

    escreva ("Obrigade por usar o App! Seu filho de uma cadela!!! \n")

    escreva ("PERDÃO!!! \n")
    
  }
}
